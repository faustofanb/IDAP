{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://idap.local/schemas/domain/category.json",
    "title": "Category",
    "type": "object",
    "required": [
        "id",
        "name"
    ],
    "properties": {
        "id": {
            "$ref": "../common/id.json"
        },
        "parentId": {
            "$ref": "../common/id.json"
        },
        "name": {
            "type": "string",
            "minLength": 1
        },
        "status": {
            "type": "string",
            "enum": [
                "ENABLED",
                "DISABLED"
            ]
        }
    },
    "additionalProperties": false
}